{
    "name": "textlint-website",
    "version": "13.1.2",
    "private": true,
    "homepage": "https://github.com/textlint/textlint/",
    "bugs": {
        "url": "https://github.com/textlint/textlint/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/textlint/textlint.git"
    },
    "scripts": {
        "build": "docusaurus-build",
        "examples": "docusaurus-examples",
        "publish-gh-pages": "docusaurus-publish",
        "rename-version": "docusaurus-rename-version",
        "start": "docusaurus-start",
        "test": "npm run textlint",
        "textlint": "textlint --cache -c .textlintrc ../docs/ ../.github/ ../README.md -f pretty-error",
        "textlint:fix": "textlint --fix --cache -c .textlintrc ../docs ../.github/ ../README.md",
        "docusaurus-version": "docusaurus-version",
        "write-translations": "docusaurus-write-translations"
    },
    "devDependencies": {
        "conventional-changelog-angular-all": "^1.7.0",
        "docusaurus": "^1.14.7",
        "textlint": "^13.1.2",
        "textlint-filter-rule-comments": "^1.2.2",
        "textlint-rule-eslint": "^4.0.1",
        "textlint-rule-prh": "^5.3.0",
        "textlint-rule-rousseau": "^1.4.8",
        "textlint-rule-unexpanded-acronym": "^1.2.4"
    },
    "files": [
        "bin/",
        "lib/",
        "module/",
        "src/",
        "!*.tsbuildinfo"
    ]
}
