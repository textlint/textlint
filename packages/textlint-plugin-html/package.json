    "cross-env": "^5.1.1",
    "glob": "^7.1.1",
    "mocha": "^3.2.0",
    "power-assert": "^1.4.0",
    "textlint": "^7.2.2",
    "textlint-ast-test": "^1.1.3",
    "textlint-rule-no-todo": "^2.0.0"
