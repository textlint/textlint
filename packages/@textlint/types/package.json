{
  "name": "@textlint/types",
  "version": "1.3.0",
  "description": "Type definition package for textlint",
  "keywords": [
    "definition",
    "textlint",
    "types",
    "typescript"
  ],
  "homepage": "https://github.com/textlint/textlint/tree/master/packages/@textlint/types/",
  "bugs": {
    "url": "https://github.com/textlint/textlint/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/textlint/textlint.git"
  },
  "license": "MIT",
  "author": "azu",
  "files": [
    "bin/",
    "lib/"
  ],
  "main": "lib/types/src/index.js",
  "types": "lib/types/src/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production tsc -p .",
    "test": "tsc -p ./test --noEmit",
    "clean": "rimraf lib/",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,css}\"",
    "prepublish": "npm run --if-present build",
    "watch": "tsc -p . --watch"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": false,
    "tabWidth": 4
  },
  "dependencies": {
    "@textlint/ast-node-types": "^4.2.5"
  },
  "devDependencies": {
    "@textlint/markdown-to-ast": "^6.1.6",
    "@types/mocha": "^5.2.6",
    "@types/node": "^12.7.12",
    "cross-env": "^6.0.3",
    "mocha": "^6.2.1",
    "prettier": "^1.17.0",
    "rimraf": "^3.0.0",
    "ts-node": "^8.4.1",
    "ts-node-test-register": "^8.0.1",
    "typescript": "^3.6.4",
    "unist-util-select": "^2.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
