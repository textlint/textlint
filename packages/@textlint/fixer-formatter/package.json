{
  "name": "@textlint/fixer-formatter",
  "version": "12.0.0-beta.3",
  "description": "textlint output formatter for fixer",
  "keywords": [
    "AST",
    "lint",
    "linting",
    "markdown",
    "plugable",
    "text",
    "textlint"
  ],
  "homepage": "https://github.com/textlint/textlint#readme",
  "bugs": {
    "url": "https://github.com/textlint/textlint/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/textlint/textlint.git"
  },
  "license": "MIT",
  "author": "azu",
  "main": "lib/src/index.js",
  "types": "lib/src/index.d.ts",
  "files": [
    "bin/",
    "lib/",
    "module/",
    "src/",
    "!*.tsbuildinfo"
  ],
  "scripts": {
    "build": "tsc -b && tsc -b tsconfig.module.json",
    "clean": "rimraf lib/ module/",
    "prepublish": "npm run build",
    "test": "mocha \"test/**/*-test.{js,ts}\""
  },
  "dependencies": {
    "@textlint/module-interop": "^12.0.0-beta.3",
    "@textlint/types": "^12.0.0-beta.3",
    "chalk": "^1.1.3",
    "debug": "^4.3.1",
    "diff": "^4.0.2",
    "is-file": "^1.0.0",
    "string-width": "^1.0.2",
    "strip-ansi": "^6.0.0",
    "text-table": "^0.2.0",
    "try-resolve": "^1.0.1"
  },
  "devDependencies": {
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.17.0",
    "cross-env": "^7.0.3",
    "mocha": "^8.4.0",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "ts-node-test-register": "^9.0.1",
    "typescript": "~4.2.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
