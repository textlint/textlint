{
  "name": "@textlint/text-to-ast",
  "version": "3.2.2",
  "description": "Parse plain text to AST with location info.",
  "keywords": [
    "ast",
    "textlint",
    "txt"
  ],
  "homepage": "https://github.com/textlint/textlint/tree/master/packages/@textlint/text-to-ast/",
  "bugs": {
    "url": "https://github.com/textlint/textlint/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/textlint/textlint.git"
  },
  "license": "MIT",
  "author": "azu",
  "main": "lib/index.js",
  "module": "./module/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "example": "example",
    "test": "test"
  },
  "files": [
    "bin/",
    "lib/",
    "module/",
    "src/"
  ],
  "scripts": {
    "build": "tsc -b && tsc -b tsconfig.module.json",
    "clean": "rimraf lib/ module/ tsconfig.tsbuildinfo tsconfig.module.tsbuildinfo",
    "crlf": "eolConverter crlf test/snapshots/crlf/input.txt",
    "prepublish": "npm run --if-present build",
    "pretest": "npm run crlf",
    "test": "mocha \"test/**/*.{js,ts}\"",
    "watch": "tsc -b --watch"
  },
  "dependencies": {
    "@textlint/ast-node-types": "^4.3.2"
  },
  "devDependencies": {
    "@textlint/ast-tester": "^2.2.2",
    "cross-env": "^7.0.2",
    "eol-converter-cli": "^1.0.8",
    "mocha": "^7.1.1",
    "power-assert": "^1.6.1",
    "rimraf": "^3.0.2",
    "ts-node-test-register": "^8.0.1",
    "typescript": "^3.8.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
