        "watch": "tsc -b --watch"
    },
    "dependencies": {
        "@textlint/ast-node-types": "^13.1.0"
    },
    "devDependencies": {
        "@textlint/markdown-to-ast": "^13.1.0",
        "@types/mocha": "^9.1.1",
        "@types/node": "^18.11.18",
        "cross-env": "^7.0.3",
        "mocha": "^10.2.0",
        "rimraf": "^3.0.2",
        "ts-node": "^10.9.1",
        "typescript": "~4.9.4"
    },
    "publishConfig": {
        "access": "public"
