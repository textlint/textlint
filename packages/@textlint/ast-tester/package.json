{
  "name": "@textlint/ast-tester",
  "version": "2.1.5",
  "description": "Compliance tests for textlint's AST(Abstract Syntax Tree).",
  "keywords": [
    "ast",
    "suite",
    "test",
    "testing",
    "textlint"
  ],
  "homepage": "https://github.com/textlint/textlint/tree/master/packages/@textlint/ast-tester",
  "bugs": {
    "url": "https://github.com/textlint/textlint/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/textlint/textlint.git"
  },
  "license": "MIT",
  "author": "azu",
  "files": [
    "src/",
    "lib/"
  ],
  "main": "lib/ast-tester/src/index.js",
  "types": "lib/ast-tester/src/index.d.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production tsc -p .",
    "clean": "rimraf lib/",
    "prepublish": "npm run --if-present build",
    "test": "mocha \"test/**/*.{js,ts}\"",
    "watch": "tsc -p . --watch"
  },
  "dependencies": {
    "@textlint/ast-node-types": "^4.2.4"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.6",
    "@types/node": "^12.7.12",
    "cross-env": "^6.0.3",
    "mocha": "^6.2.1",
    "rimraf": "^3.0.0",
    "ts-node": "^8.4.1",
    "ts-node-test-register": "^8.0.1",
    "typescript": "^3.6.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "email": "azuciao@gmail.com"
}
