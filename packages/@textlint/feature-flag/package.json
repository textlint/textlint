        "prepack": "npm run --if-present build",
        "test": "mocha",
        "watch": "tsc -b --watch"
    },
    "devDependencies": {
        "@types/mocha": "^9.1.1",
        "@types/node": "^18.19.9",
        "mocha": "^10.8.1",
        "rimraf": "^3.0.2",
        "ts-node": "^10.9.2",
        "typescript": "~5.3.3"
    }
}
