{
  "name": "textlint-tester",
  "version": "4.0.5",
  "description": "testing tool for textlint rule.",
  "keywords": [
    "test",
    "testing",
    "textlint"
  ],
  "homepage": "https://github.com/textlint/textlint/tree/master/packages/textlint-tester/",
  "bugs": {
    "url": "https://github.com/textlint/textlint/issues"
  },
  "license": "MIT",
  "author": "azu",
  "files": [
    "lib",
    "src"
  ],
  "main": "textlint-tester/src/index.js",
  "types": "textlint-tester/src/indexr.d.ts",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/textlint/textlint.git"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production tsc -p .",
    "clean": "rimraf lib/ out/",
    "prepublish": "npm run --if-present build",
    "test": "mocha \"test/**/*.{js,ts}\"",
    "watch": "tsc -p . --watch"
  },
  "dependencies": {
    "@textlint/feature-flag": "^3.0.3",
    "@textlint/kernel": "^2.0.4",
    "textlint": "^10.1.2"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.46",
    "@types/node": "^9.3.0",
    "analyze-desumasu-dearu": "^3.1.0",
    "babel-cli": "^6.10.1",
    "babel-preset-env": "^1.4.0",
    "babel-preset-jsdoc-to-assert": "^4.0.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-register": "^6.9.0",
    "cross-env": "^5.1.3",
    "match-index": "^1.0.1",
    "mocha": "^4.1.0",
    "power-assert": "^1.4.1",
    "rimraf": "^2.6.2",
    "textlint": "^10.1.2",
    "textlint-rule-helper": "^2.0.0",
    "textlint-rule-max-number-of-lines": "^1.0.2",
    "textlint-rule-no-todo": "^2.0.0",
    "ts-node": "^4.1.0",
    "typescript": "^2.6.2"
  },
