{
  "name": "textlint-tester",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/textlint/textlint.git"
  },
  "author": "azu",
  "email": "azuciao@gmail.com",
  "homepage": "https://github.com/textlint/textlint/tree/master/packages/textlint-tester/",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/textlint/textlint/issues"
  },
  "version": "2.2.1",
  "description": "testing tool for textlint rule.",
  "main": "lib/index.js",
  "files": [
    "lib",
    "src"
  ],
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production babel src --out-dir lib --source-maps",
    "watch": "babel src --out-dir lib --watch --source-maps",
    "prepublish": "npm run --if-present build",
    "test": "mocha"
  },
  "keywords": [
    "textlint",
    "test",
    "testing"
  ],
  "dependencies": {
    "textlint": "^8.0.1",
    "@textlint/feature-flag": "1.0.0"
  },
  "devDependencies": {
    "analyze-desumasu-dearu": "^3.1.0",
    "babel-cli": "^6.10.1",
    "babel-preset-env": "^1.4.0",
    "babel-preset-jsdoc-to-assert": "^4.0.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-register": "^6.9.0",
    "cross-env": "^4.0.0",
    "match-index": "^1.0.1",
    "mocha": "^3.3.0",
    "power-assert": "^1.4.1",
    "textlint": "^8.0.1",
    "textlint-rule-helper": "^2.0.0",
    "textlint-rule-max-number-of-lines": "^1.0.2",
    "textlint-rule-no-todo": "^2.0.0"
  }
